{
    "CORSRules": [
        {
            "AllowedHeaders": ["*"],
            "AllowedMethods": ["GET"],
            "AllowedOrigins": ["*"],
            "ExposeHeaders": [],
            "MaxAgeSeconds": 3000
        }
    ],
    "ContentType": {
        "*.html": "text/html",
        "*.css": "text/css",
        "*.js": "application/javascript",
        "*.webp": "image/webp",
        "*.svg": "image/svg+xml",
        "*.jpg": "image/jpeg",
        "*.jpeg": "image/jpeg",
        "*.png": "image/png"
    }
} 